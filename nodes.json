{
  "nodes": {
    "event-on-join": {
      "name": "Player Join Event",
      "description": "Run when a player joins the server.",
      "type": "event",
      "run-on": [
        "minecraft"
      ],
      "selectors": {
        "player": {
          "name": "Player",
          "type": "player",
          "connectorOut": true,
          "strategy": "disposeOnRun"
        }
      }
    },
    "player-send-message": {
      "name": "Send Message",
      "description": "Send the player a message.",
      "type": "action",
      "run-on": [
        "minecraft"
      ],
      "selectors": {
        "player": {
          "name": "Player",
          "type": "player",
          "connectorIn": true,
          "strategy": "disposeOnRun"
        },
        "message": {
          "name": "Message",
          "type": "string",
          "connectorIn": true,
          "strategy": "keep"
        }
      }
    },
    "variable": {
      "name": "Variable",
      "description": "Store variables in memory. These will be reset on server startup.",
      "type": "variable",
      "run-on": [
        "web",
        "minecraft"
      ],
      "selectors": {}
    }
  },
  "selector-types": {
    "blank": {
      "color": 4286743170,
      "annotations": {
        "default-value": {
          "hint": "Choose your type",
          "type": "blank"
        },
        "default-name": {
          "name": "Blank"
        }
      }
    },
    "player": {
      "implements": [
        "blank"
      ],
      "color": 4283600639,
      "annotations": {
        "default-value": {
          "hint": "Player name or UUID",
          "type": "text-input"
        },
        "default-name": {
          "name": "Player"
        }
      }
    },
    "string": {
      "implements": [
        "blank"
      ],
      "color": 4283215696,
      "annotations": {
        "default-value": {
          "hint": "Type your text",
          "type": "text-input"
        },
        "default-name": {
          "name": "Text"
        }
      }
    }
  }
}